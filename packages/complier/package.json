{
  "name": "larksuite-sheet-control-json-complier",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "lpm-c": "./bin/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "pages": "cd src-pages && rsbuild dev",
    "pages:build": "cd src-pages && rsbuild build && rsbuild build",
    "format": "biome format --write",
    "test": "vitest run"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@microsoft/api-extractor": "^7.52.10",
    "@rsbuild/core": "^1.4.15",
    "@rsbuild/plugin-node-polyfill": "^1.4.1",
    "@rsbuild/plugin-react": "^1.3.5",
    "@rslib/core": "^0.11.2",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^22.17.0",
    "@types/yargs": "^17.0.33",
    "react-dom": "^19.1.1",
    "vitest": "^3.2.4",
    "esbuild-wasm": "0.25.9",
    "yargs": "^17.7.2"
  },
  "private": true,
  "dependencies": {
    "@adobe/css-tools": "^4.4.3",
    "@types/react": "^19.1.9",
    "esbuild": "^0.25.8",
    "react": "^19.1.1",
    "vite": "^7.1.1",
    "zod": "^4.0.17"
  }
}
